{
  "name": "examen-bot",
  "version": "2.0.0",
  "description": "Een Discord bot voor de MNM-server om mensen geluk te wensen met hun examens die dag.",
  "main": "dist/index.js",
  "engines": {
    "node": ">=16.6.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "lint": "eslint src --ext .js,.ts",
    "start": "npm run build && cross-env NODE_ENV=development node dist/index.js",
    "prod": "npm run build && cross-env NODE_ENV=production node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Olympic1/Exam-bot"
  },
  "keywords": [
    "discord",
    "bot",
    "mnm",
    "examen"
  ],
  "author": {
    "name": "Arne Peirs",
    "email": "arnepeirs@hotmail.com"
  },
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/Olympic1/Exam-bot/issues"
  },
  "homepage": "https://github.com/Olympic1/Exam-bot#readme",
  "dependencies": {
    "cron": "^1.8.2",
    "cron-validate": "^1.4.3",
    "cross-env": "^7.0.3",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "heroku-client": "^3.1.0",
    "luxon": "^2.3.0",
    "mongoose": "^6.2.2",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@types/cron": "^1.7.3",
    "@types/heroku-client": "^3.1.0",
    "@types/luxon": "^2.0.9",
    "@types/node": "^17.0.18",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "typescript": "^4.5.5"
  }
}
